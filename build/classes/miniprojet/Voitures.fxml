<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="miniprojet.VoituresController">
   <children>
      <Label layoutX="296.0" layoutY="40.0" text="Nos voitures">
         <font>
            <Font name="Arial Rounded MT Bold" size="34.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="14.0" layoutY="459.0" mnemonicParsing="false" onAction="#backButtonAction" text="Back" />
      <TableView fx:id="voitureTable" layoutX="96.0" layoutY="120.0" prefHeight="313.0" prefWidth="634.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="75.0" text="Identifiant" />
          <TableColumn fx:id="marqueColumn" prefWidth="75.0" text="Marque" />
            <TableColumn fx:id="modeleColumn" prefWidth="75.0" text="Modèle" />
            <TableColumn fx:id="transmissionColumn" prefWidth="75.0" text="Transmission" />
            <TableColumn fx:id="couleurColumn" prefWidth="75.0" text="Couleur" />
            <TableColumn fx:id="prixColumn" prefWidth="75.0" text="Prix" />
            <TableColumn fx:id="prixPenaliteColumn" prefWidth="75.0" text="Prix Pénalité" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
