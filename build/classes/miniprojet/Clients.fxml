<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="miniprojet.ClientsController">
   <children>
      <Label layoutX="339.0" layoutY="55.0" text="Clients">
         <font>
            <Font name="Arial Rounded MT Bold" size="36.0" />
         </font>
      </Label>
      <Button fx:id="getClientButton" layoutX="68.0" layoutY="192.0" mnemonicParsing="false" onAction="#getClientButtonAction" prefHeight="49.0" prefWidth="271.0" text="Afficher les clients">
         <font>
            <Font name="Arial Rounded MT Bold" size="26.0" />
         </font>
      </Button>
      <Button fx:id="addClientButton" layoutX="461.0" layoutY="192.0" mnemonicParsing="false" onAction="#addClientButtonAction" prefHeight="49.0" prefWidth="271.0" text="Ajouter un client">
         <font>
            <Font name="Arial Rounded MT Bold" size="26.0" />
         </font>
      </Button>
      <Button fx:id="deleteClientButton" layoutX="68.0" layoutY="319.0" mnemonicParsing="false" onAction="#deleteClientButtonAction" prefHeight="49.0" prefWidth="271.0" text="Supprimer un client">
         <font>
            <Font name="Arial Rounded MT Bold" size="24.0" />
         </font>
      </Button>
      <Button fx:id="editClientButton" layoutX="461.0" layoutY="319.0" mnemonicParsing="false" onAction="#editClientButtonAction" prefHeight="49.0" prefWidth="271.0" text="Modifier un client">
         <font>
            <Font name="Arial Rounded MT Bold" size="26.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="24.0" layoutY="446.0" mnemonicParsing="false" onAction="#backButtonAction" text="Back" />
   </children>
</AnchorPane>
