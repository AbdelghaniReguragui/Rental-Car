<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="miniprojet.LocationsController">
   <children>
      <Label layoutX="311.0" layoutY="43.0" text="Locations">
         <font>
            <Font name="Arial Rounded MT Bold" size="34.0" />
         </font>
      </Label>
      <Button fx:id="afficherLocationsButton" layoutX="43.0" layoutY="183.0" mnemonicParsing="false" onAction="#afficherLocationsButtonAction" prefHeight="49.0" prefWidth="331.0" text="Afficher les locations">
         <font>
            <Font name="Arial Rounded MT Bold" size="26.0" />
         </font>
      </Button>
      <Button fx:id="addLocationButton" layoutX="426.0" layoutY="183.0" mnemonicParsing="false" onAction="#addLocationButton" prefHeight="49.0" prefWidth="331.0" text="Ajouter une location">
         <font>
            <Font name="Arial Rounded MT Bold" size="26.0" />
         </font>
      </Button>
      <Button fx:id="deleteLocationButton" layoutX="43.0" layoutY="315.0" mnemonicParsing="false" onAction="#deleteLocationButtonAction" text="Supprimer une location">
         <font>
            <Font name="Arial Rounded MT Bold" size="26.0" />
         </font>
      </Button>
      <Button fx:id="EditLocationButton" layoutX="426.0" layoutY="315.0" mnemonicParsing="false" onAction="#editLocationButtonAction" prefHeight="49.0" prefWidth="331.0" text="Modifier une location">
         <font>
            <Font name="Arial Rounded MT Bold" size="26.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="22.0" layoutY="437.0" mnemonicParsing="false" onAction="#backButtonAction" text="Back" />
   </children>
</AnchorPane>
